<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.lnu.parking.mapper.ChargeMapper">
    <update id="stopCharge">
        update t_charge
        set status = 0,
            car_num = null ,
            start_time = null ,
            charge_time = null
        where id = #{id}
    </update>
</mapper>